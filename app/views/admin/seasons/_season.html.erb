<tr>
  <td><span><%= get_index_page season_counter %></span></td>
  <td><%= season.league_name %></td>
  <td><%= season.start_time.strftime(t("format_time")) %></td>
  <td><%= season.finish_time.strftime(t("format_time")) %></td>
  <td class="text-center">
    <%= link_to edit_admin_season_path(season) do %>
    <i class="fa fa-pencil fa-fw fa-update"></i>
    <% end %>   
    <%= link_to [:admin, season], method: :delete, data:{ confirm: t("you_sure")} do %>
    <i class="fa fa-trash-o fa-fw fa-delete"></i>
    <% end %>    
  </td>
</tr>
