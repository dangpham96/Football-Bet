<%= provide :title, t("list_seasons") %>
<%= link_to t("create_season"), new_admin_season_path, class: "btn btn-primary" %>
<hr class="invisible">
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary"><%= t "list_seasons" %></h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr class="text-center">
            <th><%= t ".stt" %></th>
            <th><%= t ".league" %></th>
            <th><%= t ".start_time" %></th>
            <th><%= t ".finish_time" %></th>
            <th><%= t ".action" %></th>
          </tr>
        </thead>
        <tbody>
          <%= render partial: "season", collection: @seasons %>
        </tbody>
      </table>
    </div>
  </div>
</div>
